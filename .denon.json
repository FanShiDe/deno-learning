{
  "files": [
    "./example/server.ts",
    "./example/static_server.ts"
  ],
  "quiet": false,
  "debug": true,
  "fullscreen": true,
  "extensions": [
    ".js",
    ".ts",
    ".py",
    ".json"
  ],
  "interval": 500,
  "watch": [
    "./"
  ],
  "deno_args": [
    "--allow-net",
    "--allow-read",
    "--import-map=import-map.json"
  ],
  "scripts": {
    "start_static": {
      "cmd": "./example/static_server.ts",
      "desc": "run static server"
    },
    "start": {
      "cmd": "./example/server.ts",
      "desc": "run server"
    }
  },
  "execute": {
    ".js": [
      "deno",
      "run"
    ],
    ".ts": [
      "deno",
      "run"
    ],
    ".py": [
      "python"
    ]
  },
  "allow": [
    "net",
    "read"
  ],
  "fmt": false,
  "unstable": true,
  "test": true
}